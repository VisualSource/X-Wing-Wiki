{
  "$schema": "https://www.schemastore.org/expo-52.0.0.json",
  "expo": {
    "name": "Star Wars: X-Wing Wiki",
    "slug": "x-wiki-wiki",
    "version": "1.1.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.visualsource.xwikiwiki"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/icons/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/XWingIcons.ttf",
            "./assets/fonts/BankGthd.ttf",
            "./assets/fonts/EurostileDemiOblique.ttf",
            "./assets/fonts/kimberley.otf"
          ]
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/docs"
          ]
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCanary": true
    },
    "extra": {
      "eas": {
        "projectId": "45a37985-af8e-4e21-bc26-39eddb7516e6"
      }
    },
    "owner": "visualsource"
  }
}