{
  "name": "x-wing-wiki",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "engines": {
    "node": ">=18.*.*"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
