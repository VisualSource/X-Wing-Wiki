    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "use-debounce": "^9.0.4",
    "use-local-storage": "^2.3.6"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
